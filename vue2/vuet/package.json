{
  "name": "vuet",
  "version": "0.0.6",
  "description": "Vue state management plugin",
  "main": "dist/vuet.js",
  "scripts": {
    "cross-env": "node_modules/.bin/cross-env",
    "gulp": "node_modules/.bin/gulp",
    "dev": "npm run cross-env NODE_ENV=development node examples/server",
    "dev:test": "npm run cross-env NODE_ENV=development npm run gulp",
    "test": "npm run cross-env NODE_ENV=testing npm run cross-env SERVER=true npm run gulp",
    "e2e": "npm run cross-env NODE_ENV=testing npm run cross-env SERVER=true npm run gulp e2e",
    "unit": "npm run cross-env NODE_ENV=testing npm run gulp unit",
    "lint": "npm run cross-env NODE_ENV=testing npm run gulp lint"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medevicex/vuet.git"
  },
  "homepage": "https://github.com/medevicex/vuet#readme",
  "bugs": {
    "url": "https://github.com/medevicex/vuet/issues"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue-router",
    "vuex",
    "vuet",
    "vuex"
  ],
  "author": "medevicex",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "clear": "0.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "express": "^4.15.3",
    "express-urlrewrite": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.17.1",
    "gulp-eslint": "^3.0.1",
    "gulp-testcafe": "^0.9.1",
    "jsdom": "^10.1.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.2",
    "testcafe": "^0.15.0",
    "vue": "^2.3.3",
    "vue-loader": "^12.1.1",
    "vue-router": "^2.5.3",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "vue-scroller": "^2.2.0"
  }
}
